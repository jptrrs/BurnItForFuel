<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <!--Slightly More Realistic Wool Production-->
  <Operation Class="PatchOperationConditional">
    <xpath>/Defs/ThingCategoryDef[defName = "fleeceCategory"]</xpath>
    <match Class="PatchOperationAdd">
      <xpath>/Defs/ThingDef[defName = "BurnItForFuel"]/building/fixedStorageSettings/filter/categories</xpath>
      <value>
        <li>fleeceCategory</li>
      </value>
    </match>
  </Operation>
</Patch>